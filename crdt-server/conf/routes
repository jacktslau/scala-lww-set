# Routes
# This file defines all application routes (Higher priority routes first)
# https://www.playframework.com/documentation/latest/ScalaRouting
# ~~~~

GET      /:key                crdt.controllers.LWWSetController.lookup(key: String)
POST     /:key                crdt.controllers.LWWSetController.save(key: String, isAdd: Boolean ?= true)
DELETE   /:key                crdt.controllers.LWWSetController.save(key: String, isAdd: Boolean ?= false)
